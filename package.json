{
  "name": "eregistry",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm run vite --prefix vite-ts",
    "client2": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "dev2": "concurrently \"npm run server\" \"npm run client2\"",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d",
    "git": "git add . && git commit -m \"'\" && git push",
    "backup": "mongodump --uri=\"mongodb://localhost:27017/registryapi\" --out=/backups",
    "restore": "mongorestore --uri=\"mongodb://localhost:27017/registryapi\" --dir=/backups/registryapi"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "docx": "^8.5.0",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "joi": "^17.12.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "mongodb": "^6.3.0",
    "mongoose": "^8.1.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "dotenv": "^16.3.2",
    "nodemon": "^3.0.3",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.2"
  }
}
